{
  "uid": "rag-pipeline",
  "title": "RAG Pipeline",
  "tags": ["observability", "rag", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Stage Duration per Stage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "datasource": {"type": "prometheus", "uid": "Prometheus"},
          "expr": "histogram_quantile(0.50, sum(rate(rag_stage_duration_seconds_bucket[5m])) by (le, stage))",
          "legendFormat": "p50 - {{stage}}",
          "refId": "A"
        },
        {
          "datasource": {"type": "prometheus", "uid": "Prometheus"},
          "expr": "histogram_quantile(0.95, sum(rate(rag_stage_duration_seconds_bucket[5m])) by (le, stage))",
          "legendFormat": "p95 - {{stage}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {}
        }
      }
    },
    {
      "id": 2,
      "title": "Success/Failure Rate per Stage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "datasource": {"type": "prometheus", "uid": "Prometheus"},
          "expr": "sum(rate(rag_operations_total{success=\"true\"}[5m])) by (operation)",
          "legendFormat": "Success - {{operation}}",
          "refId": "A"
        },
        {
          "datasource": {"type": "prometheus", "uid": "Prometheus"},
          "expr": "sum(rate(rag_operations_total{success=\"false\"}[5m])) by (operation)",
          "legendFormat": "Failure - {{operation}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {}
        }
      }
    },
    {
      "id": 3,
      "title": "Number of Retrieved Candidates",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "datasource": {"type": "prometheus", "uid": "Prometheus"},
          "expr": "histogram_quantile(0.50, sum(rate(rag_candidates_count_bucket[5m])) by (le, stage))",
          "legendFormat": "p50 - {{stage}}",
          "refId": "A"
        },
        {
          "datasource": {"type": "prometheus", "uid": "Prometheus"},
          "expr": "histogram_quantile(0.95, sum(rate(rag_candidates_count_bucket[5m])) by (le, stage))",
          "legendFormat": "p95 - {{stage}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {}
        }
      }
    },
    {
      "id": 4,
      "title": "Token Usage per Request",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "datasource": {"type": "prometheus", "uid": "Prometheus"},
          "expr": "histogram_quantile(0.50, rate(rag_tokens_per_request_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "datasource": {"type": "prometheus", "uid": "Prometheus"},
          "expr": "histogram_quantile(0.95, rate(rag_tokens_per_request_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "datasource": {"type": "prometheus", "uid": "Prometheus"},
          "expr": "histogram_quantile(0.99, rate(rag_tokens_per_request_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {}
        }
      }
    }
  ]
}

